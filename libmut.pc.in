prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}/@CMAKE_INSTALL_LIBEXECDIR@
libdir=${prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@
datarootdir = ${prefix}/@CMAKE_INSTALL_DATAROOTDIR@
datadir=${prefix}/@CMAKE_INSTALL_DATADIR@

Name: libmut
Description: This holds various math utility functions I find useful in many programs.
URL: https://github.com/nicmcd/libmut
Version: 0.1.0
Requires: libprim
Libs: -L${libdir} -lmut 
Cflags: -I${includedir}
